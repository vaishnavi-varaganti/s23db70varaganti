extends layout

block content
  h1= title
  - var image_names = ["jacket_image1.jpg","jacket_image2.jpg","jacket_image3.jpeg","jacket_image4.jpg","jacket_image5.png"];
  - for(i=0;i<3;i++) {
     - var randomValue = Math.floor(Math.random()*5) ;
     img(src=`./images/${image_names[randomValue]}` alt=`img${image_names[randomValue]}` style='width:300px; height:300px;')
  -}
